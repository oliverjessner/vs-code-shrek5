    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#7e8531"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#b3c250"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": ["variable", "string constant.other.placeholder", "invalid.deprecated", "keyword", "storage.type"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#b3c250"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#586502"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage"],
            "settings": {
                "foreground": "#818756"
            }
        },
        {
            "name": "Types",
            "scope": ["storage.type", "support.type"],
            "settings": {
                "foreground": "#e00013"
            }
        },
        {
            "name": "Language Constants",
            "scope": ["constant.language", "support.constant", "variable.language"],
            "settings": {
                "foreground": "#c1d15c"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "support.variable"],
            "settings": {
                "foreground": "#4dfd02"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#799a68"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type", "entity.other.inherited-class", "support.class"],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#91f866"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#4dfd02"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": ["constant.numeric", "constant.character", "constant"],
            "settings": {
                "foreground": "#4dfd02"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#4dfd02"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#4dfd02"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#5c0b0bae"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#6c2a1d"
            }
        },
        {
            "name": "HTML: Entities",
            "scope": ["constant.character.entity", "punctuation.definition.entity"],
            "settings": {
                "foreground": "#af6728"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": ["meta.property-name", "support.type.property-name"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": ["meta.property-value", "meta.property-value constant.other", "support.constant.property-value"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Output",
            "scope": ["markup.output", "markup.raw"],
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#4B83CD"
            }
        },
        {
            "name": "Markup Styling",
            "scope": ["markup.bold", "markup.italic"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#448C27"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "foreground": "#448C27"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "foreground": "#448C27"
            }
        }
    ]
}
